age = int(input('digite sua idade: '))

if age >= 18: print('voce e maior de idade') print('voce esta no bloco if') else: print('voce e menor de idade') print('voce esta no bloco else')

print(100*'_')

nick = input('digite seu nome: ').upper()

print(f'seu nome é {nick}')

print(100*'_')

